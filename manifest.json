{
   "action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
   },
   "background": {
      "service_worker": "js/background.js"
   },
   "content_scripts": [ {
      "js": [ "js/vendor.js", "js/content_script.js", "js/font-loader.js" ],
      "css": [ "css/font-styles.css" ],
      "matches": [ "https://gemini.google.com/*" ],
      "all_frames": true
   } ],
   "default_locale": "en",
   "description": "__MSG_appDesc__",
   "host_permissions": [ "https://*.googleusercontent.com/*", "https://*.google.com/*" ],
   "icons": {
      "128": "icon.png",
      "16": "icon.png",
      "48": "icon.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "__MSG_appName__",
   "options_page": "options.html",
   "permissions": [ "storage", "downloads" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.0",
   "web_accessible_resources": [ {
      "matches": [ "https://gemini.google.com/*" ],
      "resources": [ "font.ttf", "css/font-styles.css", "js/font-loader.js", "page_texture.jpg" ]
   } ]
}
